{
  "name": "veepshosting-rsnapshot",
  "version": "2.2.0",
  "author": "grant-veepshosting",
  "summary": "Secure and easy backups with rsnapshot.",
  "license": "MIT",
  "source": "https://github.com/Veeps-Hosting/puppet-rsnapshot",
  "project_page": "https://github.com/Veeps-Hosting/puppet-rsnapshot",
  "issues_url": "https://github.com/Veeps-Hosting/puppet-rsnapshot/issues",
  "dependencies": [
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">= 3.0.0 <= 9.9.9"
    },
    {
      "name": "saz/sudo",
      "version_requirement": ">= 6.0.0 <= 9.9.9"
    },
    {
      "name": "saz/ssh",
      "version_requirement": ">= 2.3.0  <= 9.9.9"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "9"
      ]
    },
    {
